generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model ProductCategory {
  id          Int      @id @default(autoincrement())
  name        String
}

model Product {
  id          Int      @id @default(autoincrement())
  categoryId  Int
  name        String
  description String
  price       Float
  imageUrl    String
  createdAt   DateTime @default(now())
}
